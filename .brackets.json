{
    "brackets-file-tree-exclude.excludeList": [
        "/.git/",
        "/dist/",
        "/bower_components/",
        "/node_modules/"
    ],
    "jslint.options": {
        "browser": true,
        "predef": [
            "Gravatar",
            "Meteor",
            "_",
            "$",
            "Perguntas",
            "Mongo",
            "angular",
            "require",
            "describe",
            "it",
            "expect",
            "beforeEach",
            "afterEach",
            "module",
            "inject"
        ]
    }
	"highlightMatches": true,
}
